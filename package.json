{
  "name": "@google-cloud/rcloadenv",
  "description": "Node.js implementation of rcloadenv. Wraps execution of a given command and loads variables from the Google Cloud Runtime Config API into that process.",
  "version": "0.2.1",
  "license": "Apache-2.0",
  "author": "Google Inc.",
  "repository": "googleapis/nodejs-rcloadenv",
  "engines": {
    "node": ">=6"
  },
  "main": "build/src/index.js",
  "bin": {
    "rcloadenv": "./build/src/rcloadenv.js"
  },
  "scripts": {
    "lint": "eslint '**/**.js'",
    "test": "nyc mocha",
    "fix": "eslint '**/**.js' --fix",
    "samples-test": "cd samples/ && npm link ../ && npm test && cd ../",
    "system-test": "mocha system-test/ --timeout 600000",
    "docs": "jsdoc -c .jsdoc.js"
  },
  "dependencies": {
    "google-auth-library": "^2.0.1",
    "got": "^9.3.2",
    "lodash.snakecase": "^4.1.1",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "codecov": "^3.1.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "power-assert": "^1.6.1",
    "prettier": "^1.15.2"
  }
}
