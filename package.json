{
  "name": "@google-cloud/rcloadenv",
  "description": "Node.js implementation of rcloadenv. Wraps execution of a given command and loads variables from the Google Cloud Runtime Config API into that process.",
  "version": "2.0.0",
  "license": "Apache-2.0",
  "author": "Google Inc.",
  "repository": "googleapis/nodejs-rcloadenv",
  "engines": {
    "node": ">=10"
  },
  "main": "./build/src/index.js",
  "types": "./build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "bin": {
    "rcloadenv": "./build/src/cli.js"
  },
  "scripts": {
    "lint": "gts check",
    "test": "c8 mocha build/test",
    "fix": "gts fix",
    "samples-test": "cd samples/ && npm link ../ && npm test && cd ../",
    "presystem-test": "npm run compile",
    "system-test": "mocha build/system-test --timeout 600000",
    "docs": "compodoc src/",
    "compile": "tsc -p .",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "docs-test": "linkinator docs",
    "predocs-test": "npm run docs",
    "prelint": "cd samples; npm link ../; npm install",
    "clean": "gts clean",
    "precompile": "gts clean"
  },
  "dependencies": {
    "google-auth-library": "^6.0.0",
    "lodash.snakecase": "^4.1.1",
    "yargs": "^15.0.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.9",
    "@types/lodash.snakecase": "^4.1.4",
    "@types/mocha": "^7.0.0",
    "@types/nock": "^10.0.0",
    "@types/node": "^10.12.10",
    "@types/proxyquire": "^1.3.28",
    "@types/yargs": "^15.0.0",
    "c8": "^7.0.0",
    "codecov": "^3.1.0",
    "gts": "^2.0.0",
    "linkinator": "^2.0.0",
    "mocha": "^8.0.0",
    "nock": "^13.0.0",
    "proxyquire": "^2.1.0",
    "typescript": "^3.8.3"
  }
}
