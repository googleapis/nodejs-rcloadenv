{
  "name": "@google-cloud/rcloadenv",
  "description": "Node.js implementation of rcloadenv. Wraps execution of a given command and loads variables from the Google Cloud Runtime Config API into that process.",
  "version": "0.3.3",
  "license": "Apache-2.0",
  "author": "Google Inc.",
  "repository": "googleapis/nodejs-rcloadenv",
  "engines": {
    "node": ">=6"
  },
  "main": "./build/src/index.js",
  "types": "./build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "bin": {
    "rcloadenv": "./build/src/cli.js"
  },
  "scripts": {
    "lint": "gts check && eslint '**/**.js'",
    "test": "nyc mocha build/test",
    "fix": "gts fix && eslint '**/**.js' --fix",
    "samples-test": "cd samples/ && npm link ../ && npm test && cd ../",
    "presystem-test": "npm run compile",
    "system-test": "mocha build/system-test --timeout 600000",
    "docs": "compodoc src/",
    "compile": "tsc -p .",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "docs-test": "linkinator docs -r --skip www.googleapis.com",
    "predocs-test": "npm run docs"
  },
  "dependencies": {
    "google-auth-library": "^3.0.0",
    "lodash.snakecase": "^4.1.1",
    "yargs": "^13.0.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.7",
    "@types/lodash.snakecase": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.12.10",
    "@types/proxyquire": "^1.3.28",
    "@types/yargs": "^13.0.0",
    "codecov": "^3.1.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "gts": "^0.9.0",
    "mocha": "^6.0.0",
    "nock": "^10.0.2",
    "nyc": "^13.1.0",
    "power-assert": "^1.6.1",
    "prettier": "^1.15.2",
    "proxyquire": "^2.1.0",
    "source-map-support": "^0.5.9",
    "typescript": "~3.4.0",
    "linkinator": "^1.1.2"
  }
}
